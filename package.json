{
  "name": "levelgraph-jsonld",
  "version": "0.5.0",
  "description": "The Object Document Mapper for LevelGraph based on JSON-LD",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --recursive test",
    "zuul": "zuul -- test",
    "zuul-local": "zuul --local -- test/*.js",
    "coverage": "rm -rf coverage; istanbul cover _mocha -- --recursive --reporter spec --bail",
    "publish-coverage": "cat coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mcollina/levelgraph-jsonld.git"
  },
  "bugs": {
    "url": "http://github.com/mcollina/levelgraph-jsonld/issues"
  },
  "keywords": [
    "object",
    "document",
    "mapper",
    "odm",
    "graph",
    "graph",
    "database",
    "json",
    "json-ld",
    "rdf",
    "linked data"
  ],
  "author": "Matteo Collina <hello@matteocollina.com>",
  "license": "MIT",
  "dependencies": {
    "jsonld": "~0.4.6",
    "uuid": "~2.0.1",
    "async": "~1.5.2",
    "n3": "~0.4.5"
  },
  "peerDependencies": {
    "levelgraph": "^1.3.1"
  },
  "devDependencies": {
    "browserify": "~13.0.0",
    "chai": "~3.5.0",
    "coveralls": "~2.11.8",
    "istanbul": "~0.4.2",
    "level-test": "2.0.0",
    "leveldown": "~1.2.2",
    "levelgraph": "^1.3.1",
    "lodash": "~2.4.1",
    "mocha": "~2.4.5",
    "uglify-js": "~2.6.2",
    "zuul": "~3.10.1"
  }
}
